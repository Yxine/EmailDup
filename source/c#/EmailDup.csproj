<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<StartupObject>Program</StartupObject>
		<ApplicationIcon>EmailDup.ico</ApplicationIcon>
		<Description>E-mail Duplicates Cleaner</Description>
		<Product>E-mail Duplicates Cleaner</Product>
		<Company>Yxinity Group</Company>
		<Authors>Larin Aleksandr</Authors>
		<AssemblyVersion>2024.08.11.8</AssemblyVersion>
		<FileVersion>2024.08.11.8</FileVersion>
		<Version>2024.08.11.8</Version>
		<Copyright>Copyright Â© Yxinity Group, 2019-2024</Copyright>
		<PackageTags>email</PackageTags>
		<Trimming>full</Trimming>
		<PublishAot>True</PublishAot>
		<RepositoryUrl>https://github.com/Yxine/EmailDup</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<AssemblyName>EmailDup</AssemblyName>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DocumentationFile>EmailDup.xml</DocumentationFile>
		<DebugType>full</DebugType>
		<DebugSymbols>true</DebugSymbols>
		<PlatformTarget>x64</PlatformTarget>
		<DefineConstants>DEBUG</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<PlatformTarget>x64</PlatformTarget>
		<GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
		<DebugType>none</DebugType>
		<DebugSymbols>false</DebugSymbols>
		<DefineConstants />
	</PropertyGroup>
	<ItemGroup>
	  <None Remove="clean.cmd" />
	  <None Remove="EmailDup.ico" />
	  <None Remove="EmailDup.png" />
	</ItemGroup>
</Project>
